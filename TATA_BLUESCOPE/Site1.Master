</head>
<body>
    <form id="form1" runat="server">
        <div>
            <nav>
                <asp:LinkButton runat="server" ID="logo_linkbtn"><img src="images/logo.png" id ="logo"/></asp:LinkButton>
                <asp:LinkButton runat="server" Style="color: transparent">.</asp:LinkButton>
                <asp:LinkButton ID="logout" runat="server" CssClass="nav-element" Visible="True" OnClick="logout_Click">Logout</asp:LinkButton>
                <asp:LinkButton runat="server" class="nav-element" ID="adminlogin" OnClick="adminlogin_Click">Admin Login</asp:LinkButton>
                <asp:LinkButton runat="server" class="nav-element nav" ID="report">Report</asp:LinkButton>
