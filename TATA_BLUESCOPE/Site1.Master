            </nav>


            <asp:ContentPlaceHolder ID="main" runat="server">
            </asp:ContentPlaceHolder>

            <footer>
                <marquee style="color: white">&copy; TATA BLUESCOPE.</marquee>
            </footer>
        </div>
    </form>
